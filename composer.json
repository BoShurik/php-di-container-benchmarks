{
    "name": "kocsismate/di-container-benchmarks",
    "description": "Benchmarks of some popular PHP DI Containers",
    "license": "MIT",
    "homepage": "https://github.com/kocsismate/php-di-benchmarks",
    "keywords": ["Dependency Injection Container benchmarks", "Dependency Injection Container performance", "DI"],
    "authors": [
        {
            "name": "Máté Kocsis",
            "email": "kocsismate@woohoolabs.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "aura/di": "^4.0.0",
        "bitexpert/disco": "^0.10.0",
        "chubbyphp/chubbyphp-container": "^1.0.0",
        "doctrine/cache": "^1.6.1",
        "illuminate/container": "^v6.6.0",
        "joomla/di": "^1.5.1",
        "jshannon63/cobalt": "^1.2.1",
        "level-2/dice": "^4.0.1",
        "opulence/ioc": "^1.1.7",
        "php-di/php-di": "^6.0.9",
        "phpixie/di": "^3.2.1",
        "pimple/pimple": "^3.2.3",
        "psr/container": "^1.0.0",
        "rdlowrey/auryn": "^1.4.2",
        "symfony/dependency-injection": "^5.0.0",
        "thecodingmachine/yaco": "^1.0.0",
        "woohoolabs/zen": "^2.8.1",
        "yiisoft/yii2": "^2.0.22",
        "zendframework/zend-servicemanager": "^3.4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19",
        "phpstan/phpstan-strict-rules": "^0.11.1",
        "squizlabs/php_codesniffer": "^3.5.2",
        "woohoolabs/coding-standard": "^2.1.1"
    },
    "autoload": {
        "psr-4": {
            "DiContainerBenchmarks\\": "src/"
        },
        "files": [
            "src/Container/PhpDi/Resource/CompiledSingletonContainer.php"
        ]
    },
    "config": {
        "bin-compat": "full",
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
